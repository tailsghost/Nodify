        <ResourceDictionary>
            <converts:HalfOffsetConverter x:Key="HalfOffsetConverter" />
            <converts:HeightWithConnectorsConverter x:Key="HeightWithConnectorsConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
